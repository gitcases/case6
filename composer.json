{
    "name": "drupol/gcd",
    "description": "Get the GCD.",
    "require-dev": {
        "drupol/php-conventions": "^1.2"
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "sort-packages": true
    },
    "extra": {
        "grumphp": {
            "config-default-path": "vendor/drupol/php-conventions/config/php7/grumphp.yml"
        }
    },
    "autoload": {
        "psr-4": {
            "drupol\\case6\\": "./src/"
        }
    }
}
